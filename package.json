{
  "name": "cypress-to-playwright-converter",
  "version": "1.0.0",
  "description": "CLI tool to convert Cypress test projects to Playwright",
  "main": "dist/index.js",
  "bin": {
    "cy2pw": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.cli.json",
    "build:full": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src tests --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cypress",
    "playwright",
    "testing",
    "migration",
    "converter",
    "cli"
  ],
  "author": "Cy-Playwright Team",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/archiver": "^6.0.3",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.5",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.19.17",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "archiver": "^7.0.1",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.9",
    "@typescript-eslint/typescript-estree": "^6.7.0",
    "bull": "^4.16.5",
    "commander": "^11.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "fs-extra": "^11.1.1",
    "glob": "^11.0.3",
    "helmet": "^8.1.0",
    "inquirer": "^12.9.6",
    "ioredis": "^5.7.0",
    "js-yaml": "^4.1.0",
    "simple-git": "^3.28.0",
    "uuid": "^9.0.1"
  }
}
